{
  "name": "generator-gsnm",
  "version": "0.0.1",
  "description": "A simple node module starter.",
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "build": "webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha test/*.spec.js",
    "test:watch": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha -w test/*.spec.js",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec test/*"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "7.1.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-es2015": "6.24.1",
    "chai": "4.1.2",
    "commitizen": "^2.9.6",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "4.12.1",
    "eslint-loader": "1.9.0",
    "istanbul": "^0.4.5",
    "mocha": "4.0.1",
    "nyc": "^11.3.0",
    "webpack": "3.8.1",
    "yargs": "10.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/write-for-CHRIST/generator-gsnm.git"
  },
  "keywords": [
    "yeoman-generator",
    "node",
    "module",
    "seed",
    "starter"
  ],
  "author": "Nam Pham <nampdn@protonmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/write-for-CHRIST/generator-gsnm/issues"
  },
  "homepage": "https://github.com/write-for-CHRIST/generator-gsnm.git",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
